//PH1
function fun(){
    let age=18;
    let name="kiran";
    let setStatus=false;
    let userDefault=undefined;
    let reponseValue=null;
    console.log(age+" "+name+" "+setStatus+" "+userDefault+" "+reponseValue);
    let oldage=age;
    age=25;
    let status=setStatus;
    setStatus=true;
    console.log(age+" "+name+" "+setStatus+" "+userDefault+" "+reponseValue);

}
const Pah1=()=>
{
return(
    <div>
        <button onClick={fun}>click</button>
    </div>
);
};
export default Pah1;
