import React from 'react';
function msg()
{
  alert("Message to the java developer");
  console.log("hi");
}
function cw1() {
  return (
    <div className="App">
    <button onClick={msg}>CW1</button>
    <br></br>
    </div>
  );
};

export default cw1;
